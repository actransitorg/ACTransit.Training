    <DataSet Name="dsCourse">
      <Query>
        <DataSourceName>dsTraining</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@CourseTypeId">
            <Value>=Parameters!CourseTypeId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@IsActive">
            <Value>=Parameters!IsActive.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT
	 cs.CourseId
	,c.Name CourseName
FROM dbo.Course c
JOIN dbo.CourseSchedule cs ON c.CourseId = cs.CourseId
WHERE ( CAST(c.CourseTypeId AS VARCHAR(10)) IN (@CourseTypeId) OR 'All' IN (@CourseTypeId) ) -- 1=Trans 2=Maint 3=Other
AND c.IsActive IN (1,@IsActive)
/*
UNION ALL
SELECT 0 CourseID
      ,'(Today''s Scheduled Courses)'
*/
ORDER BY CourseName</CommandText>
      </Query>
      <Fields>
        <Field Name="CourseId">
          <DataField>CourseId</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="CourseName">
          <DataField>CourseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsStartEndDate">
      <Query>
        <DataSourceName>dsTraining</DataSourceName>
        <CommandText>SELECT 

CAST( CAST(YEAR(CURRENT_TIMESTAMP) AS VARCHAR(4)) + '-' + CAST(MONTH(CURRENT_TIMESTAMP)-2 AS VARCHAR(2)) + '-01' AS DATE) StartDate --from begining of month

,CAST(DATEADD(s,-1,DATEADD(mm, DATEDIFF(m,0,CURRENT_TIMESTAMP)+1,0)) AS DATE) EndDate --Last day of current month</CommandText>
      </Query>
      <Fields>
        <Field Name="StartDate">
          <DataField>StartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EndDate">
          <DataField>EndDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsCourseType">
      <Query>
        <DataSourceName>dsTraining</DataSourceName>
        <CommandText>
SELECT Name ,CAST(CourseTypeId AS VARCHAR(10)) CourseTypeId ,SortOrder
FROM dbo.CourseType
UNION ALL
SELECT 'All' ,'All' ,-1
ORDER BY SortOrder</CommandText>
      </Query>
      <Fields>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CourseTypeId">
          <DataField>CourseTypeId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SortOrder">
          <DataField>SortOrder</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsDivision">
      <Query>
        <DataSourceName>dsTraining</DataSourceName>
        <CommandText>SELECT DISTINCT TraineeLocation, TraineeLocation AS TraineeLocationValue, 0 AS SortOrder  
FROM EnrollmentTraineeDetail
WHERE TraineeLocation NOT IN ('D8','PEN')
UNION ALL
SELECT 'All' ,'All' ,-1
ORDER BY SortOrder, 1</CommandText>
      </Query>
      <Fields>
        <Field Name="TraineeLocation">
          <DataField>TraineeLocation</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SortOrder">
          <DataField>SortOrder</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TraineeLocationValue">
          <DataField>TraineeLocationValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
